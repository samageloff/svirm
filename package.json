{
  "name": "Muzli-UI-Interactions",
  "version": "1.0.0",
  "description": "A series of UI interactions, inspired from Muzli-featured designers, and implemented using modern web technologies.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samageloff/muzli-design-interactions.git"
  },
  "bugs": {
    "url": "https://github.com/samageloff/muzli-design-interactions/issues"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": "8.9.3",
    "npm": "5.6.0"
  },
  "author": "Sam Ageloff",
  "main": "server.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "start": "node ./server",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "clean": "rimraf dist",
    "build": "npm run clean && export NODE_ENV=production && webpack --config webpack.prod.js --progress --profile --colors",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "modulePaths": [
      "/src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^src/(.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@babel/preset-react": "7.0.0",
    "antd": "^3.26.2",
    "autoprefixer": "^6.7.6",
    "axios": "^0.17.1",
    "babel-cli": "6.26.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^3.8.1",
    "postcss-loader": "^2.0.9",
    "prop-types": "~15.6.0",
    "react-hot-loader": "^3.1.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-ions": "3.0.1",
    "react-loadable": "^5.5.0",
    "react-pose": "^4.0.8",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "~3.7.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^6.0.6",
    "static-loader": "^0.1.8",
    "style-loader": "^0.19.1",
    "styled-components": "4.3.2",
    "swiper": "^4.0.7",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-react-hmre": "^1.1.0",
    "chai": "^4.1.2",
    "chai-immutable": "^1.6.0",
    "clean-webpack-plugin": "^0.1.17",
    "dotenv": "^4.0.0",
    "envify": "^4.1.0",
    "enzyme": "~3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "identity-obj-proxy": "^3.0.0",
    "ions": "^0.1.2-beta-50",
    "jest": "^22.0.4",
    "jsdom": "^11.5.1",
    "json-loader": "^0.5.4",
    "map-obj": "^2.0.0",
    "node-sass": "~4.7.2",
    "react": "^16.8.6",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.1.1",
    "redux-mock-store": "^1.3.0",
    "redux-saga": "^1.0.5",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.8.0",
    "transform-loader": "^0.2.3"
  }
}
